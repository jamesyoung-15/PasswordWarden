{
    "name": "PasswordWarden",
    "version": "0.1",
    "description": "Chrome extension that generates password and checks password strength.", 
    "content_security_policy": "script-src 'self'; object-src 'self'",
      "browser_action": {
        "default_title":"Click to use",
        "default_popup": "popup.html",
        "default_icon": 
        {
            "128": "images/PasswordWardenIcon128.png"
        }
        
        
      },
      "icons": {
        "16": "images/PasswordWardenIcon16.png",
        "32": "images/PasswordWardenIcon32.png",
        "48": "images/PasswordWardenIcon48.png",
        "128": "images/PasswordWardenIcon128.png"
      },
      
      "manifest_version": 2

  }
    